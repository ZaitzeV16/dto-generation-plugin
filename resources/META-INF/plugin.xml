<idea-plugin>
    <id>com.sashashpota.DTOGenerator</id>
    <name>DTO Generator</name>
    <version>1.0</version>
    <vendor email="support@yourcompany.com" url="http://sashashpota.com">sashashpota.com</vendor>

    <description><![CDATA[
      DTO Generator
    ]]></description>

    <change-notes><![CDATA[
      DTO Generator
    ]]>
    </change-notes>

    <idea-version since-build="173.0"/>

    <extensions defaultExtensionNs="com.intellij">
